<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/Question"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginLeft="10dp"
    android:layout_marginTop="10dp"
    android:layout_marginRight="10dp"
    android:layout_marginBottom="10dp"
    android:background="#4AC2F8"
    android:backgroundTint="#55C9FD"
    android:textAlignment="center"
    tools:ignore="ExtraText"
    android:context=".activity_que_t,.Question_t_Ruby"

    >


    <TextView
        android:id="@+id/textView"
        android:layout_width="348dp"
        android:layout_height="100dp"
        android:layout_marginTop="228dp"
        android:background="#FFFFFF"
        android:fontFamily="sans-serif-black"
        android:text="        1.) What is the output of below code ? x = 20      x *=6     puts x     =begin     This is the 1 st      question you      take.     =end      "
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="#0E0E0E"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/textView2"
        app:layout_constraintHorizontal_bias="0.646"
        app:layout_constraintStart_toStartOf="@+id/textView2"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints"
        tools:text="        1.) What is the output of below code ?
         x = 20
         x *=6
         puts x
         =begin     This is the 1 st      question you      take.     =end      " />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="193dp"
        android:layout_height="38dp"
        android:layout_marginStart="90dp"
        android:layout_marginEnd="50dp"
        android:layout_marginBottom="28dp"
        android:background="#A4DEF8"
        android:fontFamily="sans-serif-condensed"
        android:text="           1/10"
        android:textColor="#111212"
        android:textSize="30dp"
        app:layout_constraintBottom_toTopOf="@+id/textView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.61"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageFilterView3"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="MissingConstraints"
        tools:text="      1/10" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="142dp"
        android:layout_height="26dp"
        android:layout_marginStart="80dp"
        android:layout_marginTop="25dp"
        android:layout_marginEnd="56dp"
        android:layout_marginBottom="30dp"
        android:background="#607E8C"
        android:fontFamily="sans-serif-black"
        android:text="      Time: 00"
        android:textColor="#090000"
        android:textSize="16dp"
        app:layout_constraintBottom_toTopOf="@+id/textView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageFilterView3"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <androidx.constraintlayout.utils.widget.ImageFilterView
        android:id="@+id/imageFilterView3"
        android:layout_width="78dp"
        android:layout_height="96dp"
        android:layout_marginStart="2dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="40dp"
        android:background="#FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/textView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo" />

    <CheckBox
        android:id="@+id/checkBox"
        android:layout_width="275dp"
        android:layout_height="37dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="30dp"
        android:background="@android:color/holo_green_dark"
        android:backgroundTint="#0DDC16"
        android:fontFamily="sans-serif-black"
        android:text="   120"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="#050505"
        app:layout_constraintEnd_toEndOf="@+id/textView"
        app:layout_constraintStart_toStartOf="@+id/textView"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        tools:text="   120" />

    <CheckBox
        android:id="@+id/checkBox3"
        android:layout_width="275dp"
        android:layout_height="37dp"
        android:layout_marginTop="5dp"
        android:background="@color/design_default_color_error"
        android:backgroundTint="#F8F6F6"
        android:fontFamily="sans-serif-black"
        android:text="   124"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="#070707"
        app:layout_constraintEnd_toEndOf="@+id/checkBox"
        app:layout_constraintStart_toStartOf="@+id/checkBox"
        app:layout_constraintTop_toBottomOf="@+id/checkBox"
        tools:text="   124" />

    <CheckBox
        android:id="@+id/checkBox4"
        android:layout_width="275dp"
        android:layout_height="37dp"
        android:layout_marginTop="5dp"
        android:background="@color/white"
        android:backgroundTint="#F43737"
        android:fontFamily="sans-serif-black"
        android:text="   126"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="#0B0B0B"
        app:layout_constraintEnd_toEndOf="@+id/checkBox3"
        app:layout_constraintStart_toStartOf="@+id/checkBox3"
        app:layout_constraintTop_toBottomOf="@+id/checkBox3"
        tools:text="   126" />

    <CheckBox
        android:id="@+id/checkBox5"
        android:layout_width="275dp"
        android:layout_height="37dp"
        android:layout_marginTop="5dp"
        android:background="#FFFFFF"
        android:backgroundTint="#FFFFFF"
        android:fontFamily="sans-serif-black"
        android:text="129"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="#090909"
        app:layout_constraintEnd_toEndOf="@+id/checkBox4"
        app:layout_constraintStart_toStartOf="@+id/checkBox4"
        app:layout_constraintTop_toBottomOf="@+id/checkBox4"
        tools:text="    129" />

    <Button
        android:id="@+id/Button1"
        android:layout_width="152dp"
        android:layout_height="69dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="13dp"
        android:fontFamily="sans-serif-black"
        android:text="Confirm"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/checkBox5"
        tools:text=" Confirm" />

    <Button
        android:id="@+id/CancelBtn"
        android:layout_width="152dp"
        android:layout_height="69dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="13dp"
        android:fontFamily="sans-serif-black"
        android:text="Cancel"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:layout_constraintStart_toEndOf="@+id/Button1"
        app:layout_constraintTop_toBottomOf="@+id/checkBox5"
        tools:text=" Cancel" /><![CDATA[




    @style/TextAppearance.AppCompat.Large"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="440dp"
        tools:ignore="MissingConstraints" />

        ]>
    ]]>
</androidx.constraintlayout.widget.ConstraintLayout>

